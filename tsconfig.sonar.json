{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"declaration": false,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"lib": [
			"es2017",
			"dom"
		],
		"paths": {
			"stream": [
				"../node_modules/readable-stream/readable.js"
			]
		},
		"outDir": "./dist/out-tsc",
		"module": "es2015",
		"moduleResolution": "node",
		"sourceMap": true,
		"target": "es2015",
		"typeRoots": [
			"node_modules/@types",
			"./src/typings"
		],
	}
}
